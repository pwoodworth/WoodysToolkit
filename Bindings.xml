<Bindings>
    <Binding name="WOODYSMOUSELOCKTOGGLE" description="Toggle MouseLook Lock" header="WOODYSTOOLKIT">
        WoodysToolkit_ToggleBindingImpl()
    </Binding>
    <Binding name="WOODYSMOUSELOCKSTART" description="Enable MouseLook Lock" header="WOODYSTOOLKIT">
        WoodysToolkit_StartBindingImpl()
    </Binding>
    <Binding name="WOODYSMOUSELOCKSTOP" description="Disable MouseLook Lock" header="WOODYSTOOLKIT">
        WoodysToolkit_StopBindingImpl()
    </Binding>
    <Binding name="WOODYSMOUSELOCKPAUSE" description="Brings up the pointer while held down" runOnUp="true" hidden="false" header="WOODYSTOOLKIT">
        WoodysToolkit_PauseBindingImpl(keystate)
    </Binding>
</Bindings>
