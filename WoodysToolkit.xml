    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Script file="WoodysToolkit.lua"/>
    <Script file="WoodysToolkitConfig.lua"/>
    <Frame name="WoodysToolkitFrame" hidden="false" parent="UIParent">
        <Scripts>
            <OnLoad function="WoodysToolkit_OnLoad"/>
            <OnEvent function="WoodysToolkit_OnEvent"/>
        </Scripts>
    </Frame>
    <Frame name="WoodysToolkitConfigFrame" hidden="true">
        <Scripts>
            <OnLoad>
                WoodysToolkitConfig_OnLoad(self);
            </OnLoad>
        </Scripts>
        <Frames>
        </Frames>
    </Frame>
</Ui>
